sim:/top/tb_mem_add \
sim:/top/tb_mem_data \
sim:/top/tb_port0 \
sim:/top/tb_port1 \
sim:/top/tb_port2 \
sim:/top/tb_port3 \
sim:/top/tb_ready_0 \
sim:/top/tb_ready_1 \
sim:/top/tb_ready_2 \
sim:/top/tb_ready_3 \
sim:/top/tb_read_0 \
sim:/top/tb_read_1 \
sim:/top/tb_read_2 \
sim:/top/tb_read_3
run -all
